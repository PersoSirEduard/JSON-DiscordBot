{
	"intends": {
		"self": [
			"i am",
			"i'm",
			"im"
		],
		"ask": [
			"tell",
			"say",
			"give",
			"answer",
			"buy"
		],
		"request": [
			"want",
			"request",
			"need",
			"desire",
			"demand"
		],
		"sad": [
			"sad",
			"depressed",
			"down",
			"gloomy",
			"bad",
			"trash",
			"heartbroken",
			"sorry",
			"distressed",
			"distress",
			"down"
		],
		"bot": [
			"dad",
			"bot",
			"father",
			"my lord",
			"dada",
			"daddy"
		],
		"comedy": [
			"joke",
			"funny"
		],
		"animal": [
			"cat",
			"dog",
			"hamster",
			"fish"
		],
		"family_portrait": [
			"family portrait",
			"family picture",
			"family selfie",
			"family photoraph"
		],
		"pepsi": ["pepsi"]
	},
	"uniques": {
		"guess who's back": "Back again!"
	},
	"actions": {
		"sad_response": {
			"intends": ["self", "sad"],
			"responseType": "list",
			"responseFile": "cheer_up.txt",
			"responseChoice": "random"
		},
		"dake_joke": {
			"intends": ["ask", "comedy", "bot"],
			"responseType": "list",
			"responseFile": "jokes.txt",
			"responseChoice": "random"
		},
		"request_animal": {
			"intends": ["request", "bot", "animal"],
			"responseType": "text",
			"responseFormat": {
				"animal": {
					"extract_from_intend": "animal"
				}
			},
			"response": "Not a chance, $user. You know very well what your mother would say if I buy you a %animal"
		},
		"sad_dad": {
			"intends": ["bot", "sad"],
			"responseType": "text",
			"response": "No $user, I am not sad...",
			"file": "https://github.com/PersoSirEduard/JSON-DiscordBot/raw/main/examples/dadbot/sad_dad_bbq.png"
		},
		"family_portrait": {
			"intends": ["family_portrait", "request", "bot"],
			"responseType": "text",
			"responseFormat": {
				"output": {
					"exec": "family_portrait.py"
				}
			},
			"response": "%output"
		},
		"pepsi": {
			"intends": ["pepsi"],
			"responseType": "text",
			"responseRate": 0.2,
			"file": "https://github.com/PersoSirEduard/JSON-DiscordBot/raw/main/examples/dadbot/pepsi.png",
			"response": ""
		},
		"i_am_joke": {
			"intends": ["self"],
			"responseType": "text",
			"responseFormat": {
				"msg": {
					"extract_after_intend": "self",
					"upper": true
				}
			},
			"response": "Hi %msg, I am dad!"
		}
	}
}
